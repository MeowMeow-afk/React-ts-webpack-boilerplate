{
  "name": "react-ts-webpack-boilerplate",
  "version": "1.0.0",
  "description": "This is a boilerplate for webpack, react and typescript from scratch ",
  "main": "index.tsx",
  "sideEffects": false,
  "scripts": {
    "start": "webpack-dashboard -- webpack serve --config configuration/webpack/webpack.dev.js",
    "build": "webpack --config configuration/webpack/webpack.prod.js",
    "postbuild": "serve dist -p 8080",
    "checkPackageUpdate": "npm-check -u",
    "dash": "webpack-dashboard"
  },
  "author": "yash chauhan",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/plugin-transform-runtime": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "babel-loader": "^8.2.5",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "dotenv-webpack": "^7.1.0",
    "error-overlay-webpack-plugin": "^1.1.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-check": "^5.9.2",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.1",
    "style-loader": "^3.3.1",
    "svg-url-loader": "^7.1.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "url-loader": "^4.1.1",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dashboard": "^3.3.7",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0",
    "webpack-obfuscator": "^3.5.1"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "serve": "^13.0.2"
  }
}
